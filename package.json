{
  "name": "tcat",
  "version": "0.0.1",
  "description": "Type Checker for AngularJS Templates",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "tcat": "dist/src/cli.js"
  },
  "scripts": {
    "build": "npm run tsc && npm run lint && npm run test",
    "lint": "tslint -c tslint.json ./src/**/*.ts ./test/**/*.ts",
    "prepublish": "npm run build",
    "start": "node dist/src/cli.js",
    "test": "mocha --require source-map-support/register --recursive dist/test",
    "tsc": "tsc",
    "tscw": "tsc -w"
  },
  "author": "Laurence Dougal Myers",
  "license": "ISC",
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.10"
  },
  "dependencies": {
    "ajv": "5.2.2",
    "cheerio": "^1.0.0-rc.2",
    "clime": "0.5.4",
    "jade": "1.11.0",
    "monet": "0.8.10",
    "pug": "2.0.0-rc.3",
    "typescript": "2.4.2",
    "uppercamelcase": "3.0.0"
  },
  "devDependencies": {
    "@types/app-root-path": "1.2.4",
    "@types/cheerio": "^0.22.2",
    "@types/jade": "0.0.30",
    "@types/mocha": "2.2.41",
    "@types/node": "6.0.63",
    "@types/pug": "2.0.4",
    "@types/uppercamelcase": "3.0.0",
    "app-root-path": "2.0.1",
    "mocha": "3.4.2",
    "source-map-support": "0.4.11",
    "tslint": "4.4.2"
  }
}
